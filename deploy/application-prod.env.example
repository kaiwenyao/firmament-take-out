# Production secrets for Firmament Server.
# Copy to /opt/firmament/config/application-prod.env and fill values.
# test github actions

FIRMAMENT_DATASOURCE_USERNAME=your-db-username
FIRMAMENT_DATASOURCE_PASSWORD=your-db-password

FIRMAMENT_ALIOSS_ACCESS_KEY_ID=your-alioss-access-key-id
FIRMAMENT_ALIOSS_ACCESS_KEY_SECRET=your-alioss-access-key-secret

FIRMAMENT_WECHAT_APPID=your-wechat-appid
FIRMAMENT_WECHAT_SECRET=your-wechat-secret

FIRMAMENT_JWT_ADMIN_SECRET_KEY=your-admin-jwt-secret-key
FIRMAMENT_JWT_USER_SECRET_KEY=your-user-jwt-secret-key
